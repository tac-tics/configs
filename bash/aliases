alias less='less -R'
alias hexdump='hexdump -C -v'
alias gpg='gpg2'
alias python='python3'

alias rg='rg --color=always'
alias dc='docker-compose'
alias vm='VBoxManage'

function add-ssh-key() {
    eval `ssh-agent -s`
    ssh-add $1
}

f () {
    find "$PWD" -type f -name '*'"$1"'*'
}
